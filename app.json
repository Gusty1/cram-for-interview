{
  "expo": {
    "name": "面試抱佛腳",
    "slug": "cram-for-interview",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gusty_tw.cramforinterview"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/LXGWWenKaiTC-Regular.ttf",
            "./src/assets/fonts/LXGWWenKaiTC-Bold.ttf"
          ]
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": false
        }
      ],
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "129afc21-8dc9-4d76-8359-d4266332123b"
      }
    },
    "owner": "gusty_tw"
  }
}